{
  "name": "cs-411-server",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "upgrade-grading": "yarn upgrade grading; cd node_modules/grading; yarn; yarn build",
    "link-grading": "rm -rf ./node_modules/grading; ln -s $HOME/work/cs411/grading ./node_modules/grading",
    "preserve": "yarn build",
    "serve:watch": "tsc-watch --onSuccess 'node ./dist/index.js'",
    "serve": "node dist/index.js",
    "start": "npm run serve:watch"
  },
  "dependencies": {
    "@faker-js/faker": "^8.2.0",
    "@koa/bodyparser": "^5.0.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.1",
    "dotenv": "^16.3.1",
    "grading": "bsu-cs-jb/grading",
    "koa": "^2.14.2",
    "lodash-es": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.10",
    "@types/koa__cors": "^4.0.2",
    "@types/koa__router": "^12.0.3",
    "@types/lodash-es": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  }
}
